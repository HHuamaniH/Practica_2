@model CapaEntidad.ViewModel.VM_DevolucionMadera
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";



}

<script src="~/Scripts/ckeditor/ckeditor.js"></script>

@Scripts.Render("~/bundles/ManDevolucionMadera/AddEdit")
@section scripts{
    <script>
        "use strict";
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
            ManDM.init();
            ItemPoa.init();
            ItemEspDev.init();
            ItemEspHalladas.init();
            ItemBExtMaderable.init();
            ItemVertice.init();
            ItemTRAprobacion.init();
            utilSigo.blockUIGeneral();
            //Poas
            var list = [];
            @foreach(var item in Model.ListNUM_POA)
            {
                <text>

            list.push({
                NUM_POA: @item.NUM_POA,
                ESTADO_ORIGEN: "@item.ESTADO_ORIGEN",
                RegEstado: RegEstadoSigo.INITIAL
            });
                </text>
            }
            ItemPoa.dt.rows.add(list).draw();


            //Especies Devueltas
            list = [];
            @foreach(var item in Model.ListESPDEVUELTAS)
            {
                <text>
                list.push({
                    ESPECIES: "@item.ESPECIES",
                    NUM_TROZAS: "@item.NUM_TROZAS",
                    VOLUMEN: "@item.VOLUMEN",
                    COORDENADA_ESTE: "@item.COORDENADA_ESTE",
                    COORDENADA_NORTE: "@item.COORDENADA_NORTE",
                    OBSERVACION: "@item.OBSERVACION",
                    COD_SECUENCIAL: "@item.COD_SECUENCIAL",
                    COD_ESPECIES: "@item.COD_ESPECIES",
                    RegEstado: RegEstadoSigo.INITIAL
                });
            </text>
            }
            ItemEspDev.dt.rows.add(list).draw();
            //Especies Halladas
            list = [];

            @foreach(var item in Model.ListESPHALLADAS)
            {
                <text>
                list.push({
                    ESPECIES: "@item.ESPECIES",
                    NUM_TROZAS: "@item.NUM_TROZAS",
                    VOLUMEN: "@item.VOLUMEN",
                    COORDENADA_ESTE: "@item.COORDENADA_ESTE",
                    COORDENADA_NORTE: "@item.COORDENADA_NORTE",
                    OBSERVACION: "@item.OBSERVACION",
                    COD_SECUENCIAL: "@item.COD_SECUENCIAL",
                    COD_ESPECIES: "@item.COD_ESPECIES",
                    RegEstado: RegEstadoSigo.INITIAL
                });
                </text>
            }
            ItemEspHalladas.dt.rows.add(list).draw();
          //Maderable
            list = [];
            @foreach(var item in Model.ListBEXTRACCION)
            {
                <text>
                list.push({
                    ESPECIES: "@item.ESPECIES",
                    VOLUMEN_AUTORIZADO: "@item.VOLUMEN_AUTORIZADO",
                    VOLUMEN_MOVILIZADO: "@item.VOLUMEN_MOVILIZADO",
                    SALDO: "@item.SALDO",
                    OBSERVACION: "@item.OBSERVACION",
                    COD_SECUENCIAL: "@item.COD_SECUENCIAL",
                    COD_ESPECIES: "@item.COD_ESPECIES",
                    RegEstado: RegEstadoSigo.INITIAL
                });
                </text>
            }
            ItemBExtMaderable.dt.rows.add(list).draw();
            //Vertice
            list = [];
            @foreach(var item in Model.ListVERTICE)
            {
                <text>
                list.push({
                    VERTICE: "@item.VERTICE",
                    COORDENADA_ESTE: "@item.COORDENADA_ESTE",
                    COORDENADA_NORTE: "@item.COORDENADA_NORTE",
                    OBSERVACION: "@item.OBSERVACION",
                    COD_SECUENCIAL: "@item.COD_SECUENCIAL",
                    RegEstado: RegEstadoSigo.INITIAL
                });
                </text>
            }
            ItemVertice.dt.rows.add(list).draw();
            //Persona
            list = [];
            @foreach(var item in Model.ListPINFTEC)
            {
                <text>
                list.push({
                    @*NUM_INFORME: "@item.NUM_INFORME",
                    FECHA_INFORME: "@item.FECHA_INFORME",*@
                    PERSONA: "@item.PERSONA",
                    N_DOCUMENTO: "@item.N_DOCUMENTO",
                    CARGO: "@item.CARGO",
                    COD_PERSONA: "@item.COD_PERSONA",
                    COD_SECUENCIAL:"@item.COD_SECUENCIAL",
                    RegEstado: RegEstadoSigo.INITIAL
                });
                </text>
            }
            ItemTRAprobacion.dt.rows.add(list).draw();



            utilSigo.ValidaRol('@ViewBag.VAliasRol', 'btnSaveDevMadera', '@Model.ddlItemIndicadorId');

            utilSigo.unblockUIGeneral();

            ItemCTroza.init();
            ItemCTocon.init();
            ItemCArbol.init();
        });



    </script>
}
<div class="container-fluid">
    <div class="card">
        <div class="card-header text-right">
            <h5>Devolucion de Madera</h5>  
        </div>

