    _renderMAPRO._fnValidarCampos = function (fechaini, fechafin) {
        var valor = true;

        if (_renderMAPRO.frm.find("#" + fechaini).val() == null ||
            _renderMAPRO.frm.find("#" + fechaini).val().length == 0 ||
            /^\s+$/.test(_renderMAPRO.frm.find("#" + fechaini).val())) {
            valor = false;
        }
        if (_renderMAPRO.frm.find("#" + fechafin).val() == null ||
            _renderMAPRO.frm.find("#" + fechafin).val().length == 0 ||
            /^\s+$/.test(_renderMAPRO.frm.find("#" + fechafin).val())) {
            valor = false;
        }

        return valor;
    }

    _renderMAPRO.fnValidarCampos = function (fechaini, fechafin, chkFechaini) {
