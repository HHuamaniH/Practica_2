@{
    ViewBag.Title = ViewBag.TituloFormulario;
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<script src="~/Areas/General/js/ManPersonas/Index.js?v=@Guid.NewGuid()"></script>
@section scripts{
    <script>
    "use strict";
    var rol = '';
    $(document).ready(function () {
        //Validamos el rol del usuario
        utilSigo.ValidaRol('@ViewBag.VAliasRol', 'btnAdd');
    });
    </script>
}


<form id="frmManPersonas">
    @Html.Hidden("tipoFormulario", (object)ViewBag.Formulario)
