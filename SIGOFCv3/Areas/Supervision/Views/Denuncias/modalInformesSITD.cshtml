<div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
        <div class="modal-header text-small" style="cursor:move;padding:0.5rem;">
            <h5 class="modal-title">Informes de  SITD</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form id="frmItemExpediente">
                <div class="form-row d-none">
                    <div class="col-md-12 ">
                        <label for="txtUbigeo" class="text-small">Código del tramite</label>
                        <div class="input-group">
                            <input id="InptDtaBuscar" class="form-control form-control-sm" placeholder="Ingrese el Código del tramite" />
                            <div class="input-group-prepend">
                                <span class="input-group-text">
                                    <i class="fa fa-search" style="cursor:pointer;"
                                       data-toggle="tooltip" data-placement="top"
                                       title="Ingrese Nro Informe"
                                       onclick="modalDocInformeSITD.buscarInforme();">
                                    </i>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md-12">
                        <table id="grvInformesSITD" class="table responsive table-hover table-bordered" width="100%"></table>
                    </div>
                </div>
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" id="btnAgregar">Agregar</button>
            <button type="button" data-dismiss="modal" aria-label="Close" class="btn btn-primary">Cerrar</button>
        </div>
    </div>
</div>
<script src="~/Areas/Supervision/js/Denuncias/ModalInformesSITD.js?noCache=@Guid.NewGuid().ToString()"></script>