                            _renderMAPRO.fnSetValorTabla8();
                            $("#trReconsideracion").show();
                            break;*/
                    }
                }
                else _renderMAPRO.fnOcultarCampos(opc);
            }
            else {
                utilSigo.toastWarning("Aviso", "Sucedio un error, Comuníquese con el Administrador");
                _renderMAPRO.fnOcultarCampos(opc);
                console.log(data.result);
            }
        });
    }

    _renderMAPRO.fnOcultarCampos = function (opc) {
