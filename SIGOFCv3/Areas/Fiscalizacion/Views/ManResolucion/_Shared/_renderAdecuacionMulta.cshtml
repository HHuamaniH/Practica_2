@model CapaEntidad.ViewModel.VM_Resodirec
    <div id="divAdecMulta">
        <div class="card-header text-small">Otros</div>
        <div><br /></div>
        <div class="form-row" id="divRectificacion">
            <div class="form-group col-md-4">
                <label for="chkGTF" class="text-small">Nuevo Importe Multa U.I.T.</label>
            </div>
            <div class="form-group col-md-6">
                @Html.EditorFor(m => m.txtAdecuMonto, new { htmlAttributes = new { @class = "form-control form-control-sm", pattern = "[0-9]*[.,]?[0-9]+", maxlength = "10", onkeypress = "return utilSigo.onKeyDecimal(event, this);" } })
            </div>
            <div class="form-group col-md-2">
            </div>
        </div>

        <!--Rectificacion-->
        <div>
            @Html.Partial("~/Areas/Fiscalizacion/Views/ManResolucion/_Shared/_generic/_renderRectificacion.cshtml", Model)
        </div>

    </div>


