        if (_renderMAPRO.frm.find("#" + chkFechaini).prop('checked')) cad = 'Del ' + _renderMAPRO.frm.find("#" + fechaini).val() + ' ';
        cad += 'Al ' + _renderMAPRO.frm.find("#" + fechafin).val();

