                                                                        <i class="fa mx-2 fa-lg fa-window-close-o" style="cursor:pointer;" onclick="ManInforme_AddEditConservacion.fnDeleteAllConservacion('ACT_TURISTICA');" data-toggle="tooltip" data-placement="top" title="Eliminar Todos"></i>
                                                                        <strong>
                                                                            Actividad turística
                                                                        </strong>
                                                                    </div>
                                                                </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody></tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
