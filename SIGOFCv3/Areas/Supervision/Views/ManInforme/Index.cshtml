
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<script src="~/Areas/Supervision/js/ManInforme/Index.js?noCache=@Guid.NewGuid().ToString()"></script>
@section scripts{
    <script>
    "use strict";
    var rol = '';
    $(document).ready(function () {
        //Validamos el rol del usuario
        utilSigo.ValidaRol('@ViewBag.VAliasRol', 'btnAdd');
    });
    </script>
}
<form id="frmManInforme">
    @Html.Hidden("tipoFormulario", (object)ViewBag.Formulario)
    @Html.Hidden("titleMenu", (object)ViewBag.TituloFormulario)
    @Html.Hidden("alertaFormulario", (object)ViewBag.AlertaInicial)

    <div id="dvManInformeContenedor" class="container-fluid"></div>
</form>

<div class="modal fade hide" id="mdlManInforme_CNotificacion" tabindex="-1" role="dialog"></div>