    _renderMAPRO._fnListar = function (opc, cell, consulta, fechaini, fechafin) {
        _reporte.frm.find("#idTab").val(2);

        if (opc == 1) _reporte.frm.find("#txtnomconsulta").val("Cantidad total de informes recibidos por la DFFFS (No incluye devueltos)");
