                                                <label for="lblmodalidad" class="text-small">Modalidad</label>
                                                @Html.EditorFor(m => m.txtmodalidad, new { htmlAttributes = new { @class = "form-control form-control-sm", disabled = true } })
                                            </div>
                                            <div class="form-group col-md-4">
