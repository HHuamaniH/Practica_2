@model CapaEntidad.ViewModel.VM_Informe_Conservacion
@{
    ViewBag.Title = "AddEditConservacion";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<script src="~/Scripts/ckeditor5/ckeditor.js"></script>
<script src="~/Scripts/sigo/utility.uploadFile.js"></script>
<script src="~/Areas/Supervision/js/ManInformeConservacion/AddEditConservacion.js?V=@Guid.NewGuid().ToString()"></script>
@section scripts{
    <script>
        "use strict"; var rol = '';
    $(document).ready(function () {
        //Validamos el rol del usuario
        utilSigo.ValidaRol('@ViewBag.VAliasRol', 'btnRegistrar', '@Model.vmControlCalidad.ddlIndicadorId');
    });
    </script>
}
<script type="text/javascript">
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbCoordenadaUTM))', "DataCoordenadaUTM");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbPrograma))', "DataPrograma");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbInfraestructura))', "DataInfraestructura");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbActividadPrograma))', "DataActividadPrograma");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbManejoImpacto))', "DataManejoImpacto");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbRegFlora))', "DataRegFlora");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbRegPaisaje))', "DataRegPaisaje");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbZonificacion))', "DataZonificacion");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbEquipamiento))', "DataEquipamiento");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbActTuristica))', "DataActTuristica");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbActIntAmbiental))', "DataIntAmbiental");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbActInvestigacion))', "DataActInvestigacion");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbActVisitas))', "DataActVisitas");
    ManInforme_AddEditConservacion.fnLoadData('@Html.Raw(Json.Encode(@Model.tbActOtroPrograma))', "DataActOtroPrograma");




</script>
<style type="text/css">
    #tbActObligAmbiental_wrapper, #tbObligacionActo_wrapper, #tbEspecieAprovechaOblig_wrapper {
        padding: 0px;
    }
</style>

<div class="container-fluid">
    <div class="card">
        <div class="card-header text-right">
            @Model.lblTituloCabecera
        </div>
        <div class="card-body">
            <div class="card">
                <div class="card-header text-small">
                    <a id="btnRegistrar" href="javascript:void(0)" style="cursor:pointer;" data-toggle="tooltip" data-placement="top" title="Guardar" onclick="ManInforme_AddEditConservacion.fnSubmitForm()"><i class="fa mx-2 fa-lg fa-save"></i></a>
                    <a style="cursor:pointer;" data-toggle="tooltip" data-placement="top" title="Regresar" onclick="ManInforme_AddEditConservacion.fnReturnIndex()"><i class="fa mx-2 fa-lg fa-reply"></i></a>
                    @Model.lblTituloEstado
                </div>
                <div class="card-body">
