                                            <label for="txtUsuarioRegistro" class="text-small">Usuario que ingresó el registro</label>
                                            @Html.EditorFor(m => m.txtUsuarioRegistro, new { htmlAttributes = new { @class = "form-control form-control-sm", @disabled = "disabled" } })

                                        </div>
