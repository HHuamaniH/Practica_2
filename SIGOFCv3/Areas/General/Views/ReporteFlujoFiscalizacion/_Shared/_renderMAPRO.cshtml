    _renderMAPRO.fnValidarConsulta = function (opc, params) {
        if (opc == 2 || opc == 4 || opc == 6 || opc == 8) {
            var cantidad;
            var cadena;

            switch (opc) {
                case 2:
                    cantidad = _renderMAPRO.frm.find("#cantMAPRO1").val();
                    cadena = "Cantidad total de informes derivados a la DFFFS y pendientes de evaluar";
                    break;

                case 4:
                    cantidad = _renderMAPRO.frm.find("#cantMAPRO4").val();
                    cadena = "Cantidad de PAU iniciados y notificados, pendientes de IFI";
                    break;

                case 6:
                    cantidad = _renderMAPRO.frm.find("#cantMAPRO7").val();
                    cadena = "Cantidad de RD apeladas y resueltas";
                    break;

                case 8:
                    cantidad = _renderMAPRO.frm.find("#cantMAPRO10").val();
                    cadena = "Cantidad de RD reconsideradas y resueltas";
                    break;
            }

            if (cantidad == 0) {
                utilSigo.toastInfo('Aviso', 'No es posible efectuar la operación cuando el valor de "' + cadena +
                    '" es igual a "0"');
            }
            else _renderMAPRO.fnTraerData(opc, params);
        }
        else _renderMAPRO.fnTraerData(opc, params);
    }

    _renderMAPRO.fnTraerData = function (opc, params) {
        var url = urlLocalSigo + "General/ReporteFlujoFiscalizacion/ObtenerData";
        var option = { url: url, datos: JSON.stringify(params), type: 'POST' };

        utilSigo.fnAjax(option, function (data) {
            if (data.success) {
                var obj = data.result;

                if (obj != null) {
                    switch (opc) {
                        case 1:
                            _renderMAPRO.frm.find("#cantMAPRO1").val(obj.cantMAPRO1);
                            _renderMAPRO.fnSetValorTabla1();
                            $("#divEvalMAPRO").show();
