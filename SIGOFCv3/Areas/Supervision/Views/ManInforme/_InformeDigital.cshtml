<div id="frmInforme" class="card no-bloqueo-rol">
    <div id="informe_header" class="card-header text-small">
        <div class="h5">
            Informe Digital {{Tramite?.cCodificacion}}
            <div class="float-right">
                <button type="button" class="btn btn-link btn-sm p-0 ml-3" v-if="Informe.ESTADO == 3" onclick="_informe.IniciarFirma()">
                    <i class="fa fa-pencil fa-fw"></i>Firmar
                </button>
                <button type="button" class="btn btn-link btn-sm p-0 ml-3" v-on:click="EliminarInforme($event)" v-if="Informe.COD_INFORME_DIGITAL && Informe.ARCHIVOINFORMEDIGITAL && (Informe.ESTADO == 1)">
                    <i class="fa fa-trash-o fa-fw"></i>Eliminar informe
                </button>
                <button type="button" class="btn btn-link btn-sm p-0 ml-3" v-on:click="SeleccionarImportar($event)" v-if="Informe.COD_INFORME_DIGITAL && ((!Informe.ARCHIVOINFORMEDIGITAL && Informe.ESTADO == 1) || (Informe.ESTADO == 6)) ">
                    <i class="fa fa-cloud-upload fa-fw"></i>Importar informe
                </button>
                <input type="file" id="fileImpInfDigital" name="fileImpInfDigital" style="display:none" size="60" v-on:change="SeleccionarArchivo(event, this)" />
                <button type="button" class="btn btn-link btn-sm p-0 ml-3" onclick="_informe.Exportar()" v-if="Informe.COD_INFORME_DIGITAL && !Informe.ARCHIVOINFORMEDIGITAL && Informe.ESTADO == 1">
                    <i class="fa fa-file-word-o fa-fw"></i>Crear informe
                </button>
                <button type="button" class="btn btn-link btn-sm p-0 ml-3" onclick="_informe.Exportar()" v-if="!Informe.ESTADO">
                    <i class="fa fa-file-word-o fa-fw"></i>preview
                </button>
                <button type="button" class="btn btn-link btn-sm p-0 ml-3 aaaaaaaa" v-on:click="DescargarInformeWord($event)" v-if="Informe.COD_INFORME_DIGITAL && Informe.ARCHIVOINFORMEDIGITAL && (Informe.ESTADO == 1 || Informe.ESTADO == 2 || Informe.ESTADO == 5 || Informe.ESTADO == 6)">
                    <i class="fa fa-file-word-o fa-fw"></i>Descargar informe
                </button>             

                <button type="button" class="btn btn-link btn-sm p-0 ml-3" onclick="_informe.Guardar()" v-if="!Informe.ESTADO  || (Informe.ESTADO == 1 &&  !Informe.ARCHIVOINFORMEDIGITAL)">
                    <i class="fa fa-save fa-fw"></i>Guardar
                </button>
                <div class="btn-group" v-if="Informe.COD_INFORME_DIGITAL && Informe.ESTADO != 5 && Informe.ESTADO != 6">
                    <button type="button" class="btn btn-link btn-sm p-0 ml-3 dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Más opciones
                    </button>
                    <div class="dropdown-menu dropdown-menu-right">
                        <template v-if="Informe.ESTADO == 1">
                            <!--<a class="dropdown-item" href="javascript:void(0);" onclick="_informe.Actualizar()" v-if="!Informe.ESTADO  || (Informe.ESTADO == 1 &&  !Informe.ARCHIVOINFORMEDIGITAL)">Actualizar informe digital</a>-->
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="javascript:void(0);" onclick="_informe.NotificarControlCalidad()">Notificar a Control de Calidad</a>
                            @*<template v-if="Tramite?.cCodificacion">
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="javascript:void(0);" onclick="_informe.CERRAR()">Cerrar informe</a>
                    </template>*@
                        </template>

                        <a class="dropdown-item" href="javascript:void(0);" onclick="_informe.SITD_OPEN()" v-if="Informe.ESTADO == 2">Generar registro SITD</a>
                        <a class="dropdown-item" href="javascript:void(0);" onclick="_informe.CARGAR_ARCHIVO()" v-if="Informe.ESTADO == 2 && Informe.TRAMITE_ID>0">Cargar documento final</a>
                        <template v-if="Informe.ESTADO == 3">
                            <a class="dropdown-item" href="javascript:void(0);" onclick="_informe.ANULAR()">Anular firmas</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="javascript:void(0);" onclick="_informe.TRANSFERIR()">Transferir documento firmado</a>
                        </template>
                        <a class="dropdown-item" href="javascript:void(0);" v-if="Informe.ESTADO == 3 || Informe.ESTADO == 4" onclick="_informe.VerDocumento()">Ver documento</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="stepwizard my-3" v-if="Informe.COD_INFORME_DIGITAL">
        <div class="stepwizard-row setup-panel">
            <div class="stepwizard-step" v-bind:class="{ 'active': Informe.ESTADO == 1 }">
                <a class="btn btn-circle" v-bind:class="Informe.ESTADO == 1 ? 'btn-success' : 'btn-secondary'">
                    <i class="fa fa-pencil"></i>
                </a>
                <p class="d-none d-sm-block">Creado</p>
            </div>
            <div v-if="Informe.ESTADO == 5" class="stepwizard-step" v-bind:class="{ 'active': Informe.ESTADO == 5 }">
                <a class="btn btn-circle" v-bind:class="Informe.ESTADO == 5 ? 'btn-success' : 'btn-secondary'">
                    <i class="fa fa-pencil"></i>
                </a>
                <p class="d-none d-sm-block">Revision</p>
            </div>
            <div v-if="Informe.ESTADO == 6" class="stepwizard-step" v-bind:class="{ 'active': Informe.ESTADO == 6 }">
                <a class="btn btn-circle" v-bind:class="Informe.ESTADO == 6 ? 'btn-success' : 'btn-secondary'">
                    <i class="fa fa-pencil"></i>
                </a>
                <p class="d-none d-sm-block">Revision-Obs</p>
            </div>
            <div class="stepwizard-step" v-bind:class="{ 'active': Informe.ESTADO == 2 }">
                <a class="btn btn-circle" v-bind:class="Informe.ESTADO == 2 ? 'btn-success' : 'btn-secondary'">
                    <i class="fa fa-lock"></i>
                </a>
                <p class="d-none d-sm-block">Cerrado</p>
            </div>
            <div class="stepwizard-step" v-bind:class="{ 'active': Informe.ESTADO == 3 }">
                <a class="btn btn-circle" v-bind:class="Informe.ESTADO == 3 ? 'btn-success': 'btn-secondary' ">
                    <i class="fa fa-cloud-upload"></i>
                </a>
                <p class="d-none d-sm-block">Archivo cargado</p>
            </div>
            <div class="stepwizard-step" v-bind:class="{ 'active': Informe.ESTADO == 4 }">
                <a class="btn btn-circle" v-bind:class="Informe.ESTADO == 4 ? 'btn-success': 'btn-secondary' ">
                    <i class="fa fa-external-link-square"></i>
                </a>
                <p class="d-none d-sm-block">Transferido SITD</p>
            </div>
        </div>
    </div>

    <div class="card-body">
        <!--<fieldset class="fs-border" v-if="Tramite?.cCodificacion">-->
        <fieldset class="fs-border" v-if="Tramite?.cCodificacion">
            <legend>Destinatario</legend>
            <div class="form-row">
                <div class="col-sm-6" v-bind:class="{'d-none': !Informe.DESTINATARIO}">
                    <div class="form-group">
                        <label class="col-form-label text-small">Para</label>
                        <div class="form-control form-control-sm" style="height: 30px;">
                            {{Informe.DESTINATARIO}}
                        </div>
                    </div>
                </div>
                <div class="col-sm-auto ml-auto">
                    <div class="form-group">
                        <label class="col-form-label text-small">Fecha SIGO</label>
                        <div class="input-group input-group-sm">
                            <date-picker v-model="Informe.FECHA"></date-picker>
                            <div class="input-group-append">
                                <div class="input-group-text">
                                    <i class="fa fa-calendar"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-auto ml-auto">
                    <div class="form-group">
                        <label class="col-form-label text-small">Fecha SITD</label>
                        <div class="input-group input-group-sm">
                            <date-picker v-model="Informe.FECHA_SITD"></date-picker>
                            <div class="input-group-append">
                                <div class="input-group-text">
                                    <i class="fa fa-calendar"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-form-label text-small">Asunto</label>
                <textarea class="form-control form-control-sm" rows="3" v-model="Informe.ASUNTO" disabled></textarea>
            </div>
            <div class="form-row">
                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="col-form-label text-small">Clave</label>
                        <div class="form-control form-control-sm" style="height: 30px;">
                            {{Informe.PASSWORD}}
                        </div>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <a href="javascript:void(0)" style="cursor:pointer;margin-top:40px;"   v-on:click="fnModalDatosRemplazar($event)" title="Ver datos a remplazar en el documento word"><i class="fa mx-2 fa-lg fa-commenting"></i></a>
                    </div>
                </div>
            </div>                
        </fieldset>
        <fieldset class="fs-border">
            <legend>Datos generales</legend>
            <div class="form-row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">Nombre del titular</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.TITULAR_SUPERVISADO" disabled />
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">DNI del titular</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.DOCUMENTO_TITULAR" disabled />
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">Representante legal</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.REPRESENTANTE_LEGAL" disabled />
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">DNI Representante legal</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.DOCUMENTO_RLEGAL" disabled />
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">N° contrato/permiso</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.NUM_THABILITANTE" disabled />
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">N° RUC Titular</label>
                        <div class="input-group input-group-sm">
                            <input type="text" class="form-control" v-model="Informe.RUC_TITULAR" disabled />
                            <div class="input-group-append">
                                <button type="button" class="btn btn-secondary" v-on:click="ConsultaRUC">
                                    <i class="fa fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">Estado RUC Titular</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.TITULAR_ESTADO_RUC" disabled />
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">Condición RUC Titular</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.TITULAR_CONDICION_RUC" disabled />
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-form-label text-small">Dirección legal</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.DIRECCION_LEGAL" disabled />
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">Nº telefónico del titular</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.TELEFONO_TITULAR" />
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label class="col-form-label text-small">Área título habilitante (ha)</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.AREA_THABILITANTE" disabled />
                    </div>
                </div>
            </div>
        </fieldset>
        <h5>Antecedentes</h5>
        <div class="form-group">
            <editor class="editor-antecedentes" v-model="Informe.ANTECEDENTES"></editor>
        </div>
        <fieldset class="fs-border mt-4">
            <legend>
                Objetivos
                <i class="fa fa-plus-circle fa-lg text-info" v-on:click="Objetivo_Agregar"></i>
            </legend>
            <div class="form-group" v-for="(item, index) in Informe.OBJETIVOS">
                <div class="input-group input-group-sm">
                    <div class="input-group-prepend">
                        <div class="input-group-text text-success">
                            <b>{{index + 1}}</b>
                        </div>
                    </div>
                    <input type="text" class="form-control" v-model="item.detalle" />
                    <div class="input-group-append">
                        <button type="button" class="btn btn-light border" v-on:click="Objetivo_Insertar(item, index)">
                            <i class="fa fa-plus-circle fa-lg text-success"></i>
                        </button>
                    </div>
                    <div class="input-group-append">
                        <button type="button" class="btn btn-light border" v-on:click="Objetivo_Eliminar(item, index)">
                            <i class="fa fa-trash fa-lg text-danger"></i>
                        </button>
                    </div>
                </div>
            </div>
            <div v-if="!Informe.OBJETIVOS.length">No hay objetivos agregados</div>
        </fieldset>
        <fieldset class="fs-border">
            <legend>Descripción del área supervisada</legend>
            <ul class="nav nav-tabs" id="tab-area" role="tablist">
                <li class="nav-item" role="presentation">
                    <a class="nav-link active" data-toggle="tab" href="#tab-ubc-politica" role="tab">Ubicación Política</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" data-toggle="tab" href="#tab-ubc-geografica" role="tab">Ubicación Geográfica</a>
                </li>
                <li class="nav-item" role="presentation" v-if="Informe.DET_TIPO != 'FAUNA' && Informe.DET_TIPO !='FAUNA_CONCESIONES'">
                    <a class="nav-link" data-toggle="tab" href="#tab-tipo-bosque" role="tab">Tipo de Bosque</a>
                </li>
            </ul>
            <div class="tab-content" id="tab-area-content">
                <div class="tab-pane fade show active" id="tab-ubc-politica" role="tabpanel">
                    <div class="form-row">
                        <div class="col-sm-7">
                            <div class="form-group">
                                <label class="col-form-label text-small">Departamento - Provincia - Distrito</label>
                                <input type="text" class="form-control form-control-sm" v-model="Informe.UBIGEO_THABILITANTE" disabled />
                            </div>
                        </div>
                        <div class="col-sm-5">
                            <div class="form-group">
                                <label class="col-form-label text-small">Sector</label>
                                <input type="text" class="form-control form-control-sm" v-model="Informe.SECTOR_THABILITANTE" disabled />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" id="tab-ubc-geografica" role="tabpanel">
                    <h6 class="mt-3">Ubicación Geográfica TH</h6>
                    <data-table :data="Informe.VERTICES_TH"
                                row-tmpl="tmpl-vertice"
                                :options="{
                                     columns: [
                                         {data: null, title: 'Vértice' },
                                         {data: 'COORDENADA_ESTE', title: 'Este'},
                                         {data: 'COORDENADA_NORTE', title: 'Norte'}
                                     ]
                                 }">
                    </data-table>
                    <div v-if="Informe.VERTICES_PC.length">
                        <h6>Ubicación Geográfica Parcela de Corta</h6>
                        <data-table :data="Informe.VERTICES_PC"
                                    row-tmpl="tmpl-vertice"
                                    :options="{
                                     columns: [
                                         {data: null, title: 'Vértice' },
                                         {data: 'COORDENADA_ESTE', title: 'Este'},
                                         {data: 'COORDENADA_NORTE', title: 'Norte'}
                                     ]
                                 }">
                        </data-table>
                    </div>
                    <script id="tmpl-vertice" type="text/x-jquery-tmpl">
                        <td class="text-center" style="width: 40px">${index + 1}</td>
                        <td>${COORDENADA_ESTE}</td>
                        <td>${COORDENADA_NORTE}</td>
                    </script>
                </div>
                <div class="tab-pane fade" id="tab-tipo-bosque" role="tabpanel">
                    <div class="form-group">
                        <label class="col-form-label text-small">Tipo de Bosque</label>
                        <input type="text" class="form-control form-control-sm" v-model="Informe.TIPO_BOSQUE" />
                    </div>
                </div>
            </div>
        </fieldset>

        <div class="row">
            <div class="col-md-6">
                <h5>Equipos <i class="fa fa-plus-circle fa-lg text-info" v-on:click="Recurso_Agregar('EQUIPOS')"></i></h5>
                <div class="mb-1" v-for="(item, index) in Informe.EQUIPOS">
                    <div class="input-group input-group-sm">
                        <div class="input-group-prepend">
                            <div class="input-group-text text-success">
                                <b>{{index + 1}}</b>
                            </div>
                        </div>
                        <input type="text" class="form-control" v-model="item.detalle" />
                        <div class="input-group-append">
                            <button type="button" class="btn btn-light border" v-on:click="Recurso_Insertar('EQUIPOS', item, index)">
                                <i class="fa fa-plus-circle fa-lg text-success"></i>
                            </button>
                        </div>
                        <div class="input-group-append">
                            <button type="button" class="btn btn-light border" v-on:click="Recurso_Eliminar('EQUIPOS', item, index, 'RECURSO')">
                                <i class="fa fa-trash fa-lg text-danger"></i>
                            </button>
                        </div>
                    </div>
                </div>
                <p v-if="!Informe.EQUIPOS.length">No hay recursos para mostrar</p>
            </div>
            <div class="col-md-6">
                <h5>Materiales <i class="fa fa-plus-circle fa-lg text-info" v-on:click="Recurso_Agregar('MATERIALES')"></i></h5>
                <div class="mb-1" v-for="(item, index) in Informe.MATERIALES">
                    <div class="input-group input-group-sm">
                        <div class="input-group-prepend">
                            <div class="input-group-text text-success">
                                <b>{{index + 1}}</b>
                            </div>
                        </div>
                        <input type="text" class="form-control" v-model="item.detalle" />
                        <div class="input-group-append">
                            <button type="button" class="btn btn-light border" v-on:click="Recurso_Insertar('MATERIALES', item, index)">
                                <i class="fa fa-plus-circle fa-lg text-success"></i>
                            </button>
                        </div>
                        <div class="input-group-append">
                            <button type="button" class="btn btn-light border" v-on:click="Recurso_Eliminar('MATERIALES', item, index, 'RECURSO')">
                                <i class="fa fa-trash fa-lg text-danger"></i>
                            </button>
                        </div>
                    </div>
                </div>
                <p v-if="!Informe.MATERIALES.length">No hay recursos para mostrar</p>
            </div>
        </div>

        <h5 class="mt-3">Detalle de la diligencia</h5>
        <ul class="nav nav-tabs" id="tab-diligencia" role="tablist">
            <li class="nav-item" role="presentation">
                <a class="nav-link active" data-toggle="tab" href="#tab-dil-1" role="tab">Acceso al área del TH</a>
            </li>
            <li class="nav-item" role="presentation">
                <a class="nav-link" data-toggle="tab" href="#tab-dil-2" role="tab">Cronograma</a>
            </li>
            <li class="nav-item" role="presentation">
                <a class="nav-link" data-toggle="tab" href="#tab-dil-3" role="tab">Participantes</a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane fade show active" id="tab-dil-1" role="tabpanel">
                <div class="mt-2 mb-2">
                    <button type="button" class="btn btn-primary btn-sm" v-on:click="Acceso_TH_Nuevo">Nuevo</button>
                </div>
                <div class="table-responsive" v-if="Informe.ACCESO_AREAS_TH.length">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th class="text-center">Tramo</th>
                                <th class="text-center">Distancia (km)</th>
                                <th class="text-center">Tiempo (horas)</th>
                                <th class="text-center">Medio de transporte</th>
                                <th class="text-center">Tipo de vehículo</th>
                                <th>Observaciones</th>
                                <th class="text-center">Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr v-for="(item, index) in Informe.ACCESO_AREAS_TH">
                                <td class="text-center">{{item.tramo}}</td>
                                <td class="text-center">{{item.distancia}}</td>
                                <td class="text-center">{{item.tiempo}}</td>
                                <td class="text-center">{{item.transporte}}</td>
                                <td class="text-center">{{item.tipoVehiculo}}</td>
                                <td>{{item.observaciones}}</td>
                                <td class="text-center align-middle">
                                    <i class="fa fa-pencil fa-2x mr-2" title="Editar" v-on:click="Acceso_TH_Editar(item, index)"></i>
                                    <i class="fa fa-trash-o fa-2x" title="Eliminar" v-on:click="Acceso_Area_Eliminar(item, index)"></i>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="tab-pane fade" id="tab-dil-2" role="tabpanel">
                <editor v-model="Informe.CRONOGRAMA" class="mt-3"></editor>
            </div>
            <div class="tab-pane fade" id="tab-dil-3" role="tabpanel">
                <table class="table">
                    <thead>
                        <tr>
                            <th>N°</th>
                            <th>Participante</th>
                            <th>Función</th>
                            <th>DNI</th>
                            <th>Observación</th>
                            <th>Opciones</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="text-center align-middle"></td>
