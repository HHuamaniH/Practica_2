    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<script src="~/Areas/THabilitante/js/ManObligaciones/ValidarObligacion.js"></script>

<script type="text/javascript">
    ValidarObligacion.fnLoadData('@Html.Raw(Json.Encode(@Model.listSenial))', 'DataSenial');
    ValidarObligacion.fnLoadData('@Html.Raw(Json.Encode(@Model.listHallazgo))', 'DataHallazgo');
    ValidarObligacion.fnLoadData('@Html.Raw(Json.Encode(@Model.listArchivoDenuncia))', 'DataArchivoDenuncia');
    ValidarObligacion.fnLoadData('@Html.Raw(Json.Encode(@Model.listArchivoOtros))', 'DataArchivoOtros');
    ValidarObligacion.fnLoadData('@Html.Raw(Json.Encode(@Model.listArchivo))', 'DataArchivo');
    ValidarObligacion.fnLoadData('@Html.Raw(Json.Encode(@Model.listEvento))', 'DataEvento');
