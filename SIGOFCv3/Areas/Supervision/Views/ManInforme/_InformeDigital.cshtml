        data.COD_INFORME = form.hdfCodInforme;
        data.FECHA_REGISTRO = new Date();
        //data.COD_THABILITANTE = data.NUM_THABILITANTE;
        data.COD_MTIPO = form.hdfCodMTipo;

        //TABLAS
        let _renderArray = function (_arr) {
            _arr.forEach(function (item, index) {
                item.orden = index + 1;
                if (item.item) {
                    item.estado = 2;
                }
            });

            return _arr;
        }

        data.OBJETIVOS = _renderArray(data.OBJETIVOS);
        data.EQUIPOS = _renderArray(data.EQUIPOS);
        data.MATERIALES = _renderArray(data.MATERIALES);
        data.ACCESO_AREAS_TH = _renderArray(data.ACCESO_AREAS_TH);
        data.PARTICIPANTES = _renderArray(data.PARTICIPANTES);
        data.ANEXOS = _renderArray(data.ANEXOS);

        let equipos = data.EQUIPOS.map(function (x) { x.tipo = 'E'; return x; });
        let materiales = data.MATERIALES.map(function (x) { x.tipo = 'M'; return x; });
        data.RECURSOS = equipos.concat(materiales);

        let parametros = {
            COD_INFORME_DIGITAL: data.COD_INFORME_DIGITAL,
            COD_INFORME: data.COD_INFORME,
            COD_THABILITANTE: data.COD_THABILITANTE,
            COD_MTIPO: data.COD_MTIPO,
            TRAMITE_ID: data.TRAMITE_ID,
            NUM_INFORME_SITD: data.NUM_INFORME_SITD,
