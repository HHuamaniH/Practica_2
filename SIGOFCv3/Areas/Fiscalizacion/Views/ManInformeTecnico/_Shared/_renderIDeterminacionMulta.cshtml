@model CapaEntidad.ViewModel.VM_InformeTecnico
<div id="divDeterminacionMulta">
    <div class="card-header text-small">Determinación Multa</div>
    <div><br /></div>
    <div class="form-row" id="divRectificacion">
        <div class="form-group col-md-4">
            <label for="chkGTF" class="text-small">Multa recomendada (UIT)</label>
        </div>
        <div class="form-group col-md-6">
            @Html.EditorFor(m => m.txtMultaRecomendUIt, new { htmlAttributes = new { @class = "form-control form-control-sm", pattern = "[0-9]*[.,]?[0-9]+", maxlength = "10", onkeypress = "return utilSigo.onKeyDecimal(event, this);" } })

        </div>
        <div class="form-group col-md-2">
        </div>
    </div>
    <div class="form-row" id="divRectificacion">
        <div class="form-group col-md-4">
            <label for="chkGTF" class="text-small">Multa recomendada (Soles)</label>
        </div>
        <div class="form-group col-md-6">
            @Html.EditorFor(m => m.txtMultaRecomendSol, new { htmlAttributes = new { @class = "form-control form-control-sm", pattern = "[0-9]*[.,]?[0-9]+", maxlength = "10", onkeypress = "return utilSigo.onKeyDecimal(event, this);" } })

        </div>
        <div class="form-group col-md-2">
        </div>
    </div>
    <div class="form-row" id="divRectificacion">
        <div class="form-group col-md-4">
            <label for="chkGTF" class="text-small">Motivo de multa</label>
        </div>
        <div class="form-group col-md-6">
            @Html.TextAreaFor(m => m.txtMotivMulta, 4, 1, new { @class = "form-control form-control-sm" })
        </div>
        <div class="form-group col-md-2">
        </div>
    </div>

    <div>
        @Html.Partial("~/Areas/Fiscalizacion/Views/ManInformeTecnico/_Shared/_renderSSupervision.cshtml", Model)
    </div>
</div>





