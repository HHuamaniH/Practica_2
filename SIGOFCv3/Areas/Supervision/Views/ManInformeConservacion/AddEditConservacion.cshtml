                                                <div class="input-group">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text"><i class="fa fa-search-plus" style="cursor:pointer;" data-toggle="tooltip" data-placement="top" title="Seleccionar titular" onclick="ManInforme_AddEditConservacion.fnBuscarPersona('TITULAR_EJECUTA','0000001');"></i></span>
                                                    </div>
                                                    @Html.EditorFor(m => m.txtTitularEjecutaPgmf, new { htmlAttributes = new { @class = "form-control form-control-sm", @disabled = "disabled" } })
                                                    @Html.HiddenFor(m => m.hdfCodTitularEjecutaPgmf)
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-3">
                                                <label for="txtRegenteImplPgmf" class="text-small">Regente Encargado de la Implementación</label>
                                                <div class="input-group">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text"><i class="fa fa-search-plus" style="cursor:pointer;" data-toggle="tooltip" data-placement="top" title="Seleccionar regente" onclick="ManInforme_AddEditConservacion.fnBuscarPersona('REGENTE_IMPLEMENTA', '0000020,0000003');"></i></span>
                                                    </div>
                                                    @Html.EditorFor(m => m.txtRegenteImplPgmf, new { htmlAttributes = new { @class = "form-control form-control-sm", @disabled = "disabled" } })
                                                    @Html.HiddenFor(m => m.hdfCodRegenteImplPgmf)
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-header text-small">Delimitación de la Concesión</div>
