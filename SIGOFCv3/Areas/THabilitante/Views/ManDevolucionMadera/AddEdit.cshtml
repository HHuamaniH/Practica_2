                                        <div class="form-group col-md-4 mb-3">
                                            <label for="txtItemEjecucionInicio" class="text-small">Fecha Inicio</label>
                                            @Html.EditorFor(m => m.txtItemEjecucionInicio, new { htmlAttributes = new { @class = "form-control form-control-sm", Maxlength = 10, Pattern = "[0-9]{2}/[0-9]{2}/[0-9]{4}", Placeholder = "dd/mm/aaaa", Title = "la fecha a ingresar debe ser en el formato dd/mm/aaaa" } })

