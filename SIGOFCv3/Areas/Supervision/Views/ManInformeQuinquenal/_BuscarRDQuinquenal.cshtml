<script src="~/Areas/Supervision/js/ManInformeQuinquenal/_BuscarRDQuinquenal.js"></script> 

@{
    ViewBag.Title = "_BuscarRDQuinquenal";
}

<div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
        <div class="modal-header text-small" style="cursor:move;padding:0.5rem;">
            <h5 id="lblTituloBuscarPersona" class="modal-title">Buscar R.D. Quinquenal</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form id="frmBuscarRDQuinquenal" method="get">
             
                <div class="form-row">
                    <div class="col-md-4 mb-3">
                        Buscar
                    </div>
                    <div class="col-md-8 mb-3">
                        <div class="input-group">
                            <input type="text" id="txtbusRDQ" class="form-control form-control-sm" maxlength="50" style="text-transform:uppercase;" />
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="btnItemBuscarRD" style="cursor:pointer;color:forestgreen">
                                <i class="fa fa-search" data-toggle="tooltip" data-placement="top" title="Buscar"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="table-responsive">
                    <table id="tbBuscarRDQuinquenal" class="table table-hover table-bordered" width="100%">
                        <thead></thead>
                        <tbody></tbody>
                    </table>
                </div>
                <button type="submit" style="display:none;"></button>
            </form>
        </div>
        <div class="modal-footer">

        </div>
    </div>
</div>
