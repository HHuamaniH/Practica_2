                dragLeave: function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                },
                drop: function (e) {
                    let dataTransfer = e.dataTransfer;
                    if (dataTransfer && dataTransfer.files.length) {
                        e.preventDefault();
                        e.stopPropagation();

                        let files = dataTransfer.files;
                        this.set(files[0]);
                    }
                },
                fileChange: function (e) {
                    let files = e.target.files;
                    if (files.length) {
                        this.set(files[0]);
                        setTimeout(function () { e.target.value = ""; }, 100);
                    }
                },
                fileSearch: function (e) {
                    $(e.target).closest('.modal-body').find('input[type=file]').click();
                },
                set: function (f) {
                    if (f.type !== 'application/pdf') {
                        utilSigo.toastWarning('Atención', 'El archivo a cargar debe ser del tipo PDF');
                        return;
                    }

                    if (f.size > 100 * 1024 * 1024) {
                        utilSigo.toastWarning('Atención', 'El archivo no debe ser mayor a 100MB');
                        return;
                    }

                    this.file = f;
                    this.error = false;
                    this.percent = 0;

                    let bytesToSize = function(x) {
                        const units = ['bytes', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];
                        let l = 0, n = parseInt(x, 10) || 0;
                        while (n >= 1024 && ++l) {
                            n = n / 1024;
                        }
                        return (n.toFixed(n < 10 && l > 0 ? 1 : 0) + ' ' + units[l]);
                    }

                    this.info = {
                        name: f.name,
                        size: f.size,
                        sizeFormat: bytesToSize(f.size),
                        type: f.type
                    }
                },
                send: function () {
                    let self = this;
                    self.error = false;
                    self.percent = 0;

