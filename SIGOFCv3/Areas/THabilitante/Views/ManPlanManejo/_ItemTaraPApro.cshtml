<div class="modal-dialog" role="document">
    <div class="modal-content" id="contenedorTaraPApro">
        <div class="modal-header text-small" style="cursor:move;padding:0.5rem;">
            <h5 class="modal-title" id="h5Titulo">Nuevo</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form id="frmTaraPApro">
                <input type="hidden" id="COD_SECUENCIAL" name="COD_SECUENCIAL" value="0" />
                <input type="hidden" id="RegEstado" name="RegEstado" value="1" />
                <input type="hidden" id="RegEstadoClient" name="RegEstadoClient" value="1" />
                <div class="form-group row">
                    <label for="ANO" class="col-md-4 col-form-label text-small">Año</label>
                    <div class="col-md-8">
                        <input type="text" id="ANO" name="ANO" class="form-control form-control-sm" placeholder="Año" maxlength="5" onkeypress="return utilSigo.onKeyEntero(event, this);" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="NUM_ARBOLES" class="col-md-4 col-form-label text-small">Número de árboles</label>
                    <div class="col-md-8">
                        <input type="text" id="NUM_ARBOLES" name="NUM_ARBOLES" class="form-control form-control-sm" maxlength="4" onkeypress="return utilSigo.onKeyEntero(event, this);" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="PESO_VAINAS" class="col-md-4 col-form-label text-small">Peso de las vainas</label>
                    <div class="col-md-8">
                        <input type="text" id="PESO_VAINAS" name="PESO_VAINAS" class="form-control form-control-sm" maxlength="7" onkeypress="return utilSigo.onKeyDecimal(event, this);" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="NUM_COSECHA" class="col-md-4 col-form-label text-small">Número de cosechas</label>
                    <div class="col-md-8">
                        <input type="text" id="NUM_COSECHA" name="NUM_COSECHA" class="form-control form-control-sm" maxlength="7" onkeypress="return utilSigo.onKeyEntero(event, this);" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="NUM_QUINTALES" class="col-md-4 col-form-label text-small">Número de quintales</label>
                    <div class="col-md-8">
                        <input type="text" id="NUM_QUINTALES" name="NUM_QUINTALES" class="form-control form-control-sm"  maxlength="7" onkeypress="return utilSigo.onKeyDecimal(event, this);" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="OBSERVACION" class="col-md-4 col-form-label text-small">Observacíon</label>
                    <div class="col-md-8">
                        <textarea id="OBSERVACION" name="OBSERVACION" class="form-control form-control-sm" rows="2" maxlength="300" />
                    </div>
                </div>
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" id="btnGuardar" class="btn btn-primary btn-sm">Agregar</button>
        </div>
    </div>
</div> 
